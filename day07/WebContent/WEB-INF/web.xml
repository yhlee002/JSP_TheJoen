<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>day07</display-name>
  <servlet>
  	<servlet-name>personCtr</servlet-name>
  	<servlet-class>day07.person.PersonCtr</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>personListCtr</servlet-name>
  	<servlet-class>day07.person.PersonListCtr</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>personCtr</servlet-name>
  	<url-pattern>/person</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>personListCtr</servlet-name>
  	<url-pattern>/personlist</url-pattern>
  </servlet-mapping>
</web-app>